{
    "name": "colbycommunications/colby-site",
    "description": "Configuration project for the colby.edu site",
    "type": "project",
    "require": {
        "johnpbloch/wordpress-core-installer": "^2.0",
        "johnpbloch/wordpress-core": "^6.0",
        "wpackagist-theme/twentyseventeen": "^2.1",
        "wpackagist-plugin/classic-editor": "^1.4",
        "wpackagist-plugin/capability-manager-enhanced": "2.3.2",
        "wpackagist-plugin/header-footer": "^3.1.3",
        "wpackagist-plugin/duplicate-post": "^4.3",
        "wpackagist-plugin/wordpress-seo": "^19.3",
        "wpackagist-plugin/cloudflare": "^4.5.1",
        "wpackagist-plugin/public-post-preview": "^2.9.1",
        "wpackagist-plugin/wp-saml-auth": "^2.0.0",
        "wpackagist-plugin/siteimprove": "1.2.1",
        "wpackagist-plugin/query-monitor": "^3.7.1",
        "wp-cli/wp-cli-bundle": "^2.4",
        "psy/psysh": "^0.10.4",
        "colbycommunications/colby-college-theme": "1.2.4",
        "colbycommunications/colby-ticket": "^1.0.0",
        "wpackagist-plugin/wp-search-with-algolia": "^2.1.0",
        "vlucas/phpdotenv": "^5.4",
        "wpackagist-plugin/press-permit-core": "^3.7.10",
        "wpackagist-plugin/user-switching": "^1.7.0",
        "colbycommunications/presspermit-pro": "^3.8.2-beta",
        "wpackagist-plugin/disable-comments": "^2.4.1",
        "wpackagist-plugin/advanced-gutenberg": "3.1.4.2",
        "wpackagist-plugin/term-management-tools": "2.0.1",
        "colbycommunications/capabilities-pro": "2.5.0",
        "wpackagist-plugin/wordpress-importer": "0.7",
        "wpackagist-plugin/custom-post-type-ui": "1.13.1",
        "colbycommunications/advanced-custom-fields-pro": "^6.0.3",
        "colbycommunications/gravityforms": "^2.6.9",
        "phpseclib/phpseclib": "~3.0",
        "wpackagist-plugin/convert-to-blocks": "^1.0.2",
        "wpackagist-plugin/shortcodes-finder": "^1.5.1",
        "wpackagist-plugin/regenerate-thumbnails": "3.1.5",
        "wpackagist-plugin/timber-library": "^1.21.0",
        "wpackagist-plugin/wp-nested-pages": "3.2.3",
        "wpackagist-plugin/block-manager": "^1.2",
        "wpackagist-plugin/tinymce-advanced": "^5.6"
    },
    "authors": [
        {
            "name": "Morty Mule",
            "email": "webmaster@colby.edu"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/ColbyCommunications/colby-college-theme"
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org/"
        },
        {
            "type": "composer",
            "url": "https://satis.colby.edu"
        }
    ],
    "extra": {
        "wordpress-install-dir": "web/wp",
        "installer-paths": {
            "web/wp-content/mu-plugins/{$name}": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/wordpress-seo"
            ],
            "web/wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ],
            "web/wp-content/themes/{$name}": [
                "type:wordpress-theme"
            ]
        }
    },
    "config": {
        "allow-plugins": true
    }
}
