{
    "name": "colbycommunications/colby-site",
    "description": "Configuration project for the colby.edu site",
    "type": "project",
    "require": {
        "johnpbloch/wordpress-core-installer": "^2.0",
        "johnpbloch/wordpress-core": "6.2.1",
        "wpackagist-plugin/capability-manager-enhanced": "2.3.2",
        "wpackagist-plugin/header-footer": "3.2.5",
        "wpackagist-plugin/duplicate-post": "4.5",
        "wpackagist-plugin/cloudflare": "4.11.0",
        "wpackagist-plugin/public-post-preview": "2.10.0",
        "wpackagist-plugin/wp-saml-auth": "2.1.3",
        "wpackagist-plugin/query-monitor": "3.12.3",
        "wp-cli/wp-cli-bundle": "^2.4",
        "psy/psysh": "^0.10.4",
        "colbycommunications/colby-college-theme": "1.7.1",
        "wpackagist-theme/twentytwenty": "2.2",
        "colbycommunications/colby-ticket": "1.0.1",
        "wpackagist-plugin/wp-search-with-algolia": "2.5.1",
        "vlucas/phpdotenv": "^5.4",
        "wpackagist-plugin/press-permit-core": "3.9.3",
        "wpackagist-plugin/user-switching": "1.7.0",
        "colbycommunications/presspermit-pro": "^3.8.2-beta",
        "wpackagist-plugin/disable-comments": "2.4.3",
        "wpackagist-plugin/advanced-gutenberg": "3.1.4.2",
        "wpackagist-plugin/term-management-tools": "2.0.1",
        "colbycommunications/capabilities-pro": "2.5.0",
        "wpackagist-plugin/wordpress-importer": "0.7",
        "wpackagist-plugin/custom-post-type-ui": "1.13.1",
        "colbycommunications/advanced-custom-fields-pro": "6.0.3",
        "colbycommunications/gravityforms": "2.6.9",
        "phpseclib/phpseclib": "~3.0",
        "wpackagist-plugin/convert-to-blocks": "1.1.1",
        "wpackagist-plugin/regenerate-thumbnails": "3.1.5",
        "wpackagist-plugin/timber-library": "1.22.1",
        "wpackagist-plugin/wp-nested-pages": "3.2.3",
        "wpackagist-plugin/tinymce-advanced": "5.9.0",
        "wpackagist-plugin/wordpress-seo": "20.8",
        "wpackagist-plugin/disable-everything": "0.4.0",
        "wpackagist-plugin/tablepress": "2.1.7"
    },
    "authors": [
        {
            "name": "Morty Mule",
            "email": "webmaster@colby.edu"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/ColbyCommunications/colby-college-theme"
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org/"
        },
        {
            "type": "composer",
            "url": "https://satis.colby.edu"
        }
    ],
    "extra": {
        "wordpress-install-dir": "web/wp",
        "installer-paths": {
            "web/wp-content/mu-plugins/{$name}": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/seo-by-rank-math"
            ],
            "web/wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ],
            "web/wp-content/themes/{$name}": [
                "type:wordpress-theme"
            ]
        }
    },
    "config": {
        "allow-plugins": true
    }
}
