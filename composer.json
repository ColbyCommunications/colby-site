{
    "name": "colbycommunications/colby-site",
    "description": "Configuration project for the colby.edu website",
    "type": "project",
    "require": {
        "johnpbloch/wordpress-core-installer": "^2.0",
        "johnpbloch/wordpress-core": "6.5.5",
        "wpackagist-plugin/header-footer": "3.2.8",
        "wpackagist-plugin/duplicate-post": "4.5",
        "wpackagist-plugin/cloudflare": "4.12.8",
        "wpackagist-plugin/public-post-preview": "2.10.0",
        "wpackagist-plugin/wp-saml-auth": "2.1.4",
        "wp-cli/wp-cli-bundle": "^2.4",
        "psy/psysh": "^0.10.4",
        "colbycommunications/colby-college-theme": "2.11.2",
        "colbycommunications/colby-ticket": "1.0.1",
        "wpackagist-plugin/wp-search-with-algolia": "2.8.1",
        "vlucas/phpdotenv": "^5.4",
        "wpackagist-plugin/user-switching": "1.8.0",
        "colbycommunications/presspermit-pro": "4.0.14",
        "wpackagist-plugin/disable-comments": "2.4.6",
        "wpackagist-plugin/advanced-gutenberg": "3.2.4",
        "wpackagist-plugin/term-management-tools": "2.0.1",
        "colbycommunications/capabilities-pro": "2.13.0",
        "wpackagist-plugin/wordpress-importer": "0.8.2",
        "colbycommunications/advanced-custom-fields-pro": "6.3.3",
        "colbycommunications/gravityforms": "2.8.13",
        "colbycommunications/colby-its-status": "1.0.0",
        "phpseclib/phpseclib": "~3.0",
        "wpackagist-plugin/regenerate-thumbnails": "3.1.6",
        "timber/timber": "^1.0",
        "wpackagist-plugin/wp-nested-pages": "3.2.8",
        "wpackagist-plugin/tinymce-advanced": "5.9.2",
        "wpackagist-plugin/wordpress-seo": "23.3",
        "wpackagist-plugin/disable-everything": "0.4.1",
        "wpackagist-plugin/tablepress": "2.4.1",
        "wpackagist-plugin/cf-images": "1.8.0"
    },
    "authors": [
        {
            "name": "Morty Mule",
            "email": "webmaster@colby.edu"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/ColbyCommunications/colby-college-theme"
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org/"
        },
        {
            "type": "composer",
            "url": "https://satis.colby.edu"
        }
    ],
    "extra": {
        "wordpress-install-dir": "web/wp",
        "installer-paths": {
            "web/wp-content/mu-plugins/{$name}": [
                "type:wordpress-muplugin",
                "wpackagist-plugin/seo-by-rank-math"
            ],
            "web/wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ],
            "web/wp-content/themes/{$name}": [
                "type:wordpress-theme"
            ]
        }
    },
    "config": {
        "allow-plugins": true
    }
}
